<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.projectlearning.mapper.MemberMapper">
	
	<select id="checkMember" parameterType="com.projectlearning.dto.MemberDTO" resultType="com.projectlearning.dto.MemberDTO">
		SELECT   id
				,password
		FROM    t_user
		WHERE   id = #{id}
			AND password = #{password}
	</select>


</mapper>